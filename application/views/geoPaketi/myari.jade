block content
	script(src='/javascripts/client/controllers/operatori/geoPaketi/axaliMyariCtrl.js')
	div
		.jumbotron.jumbotron-ultra-sm
			.container
				.row
					.col-sm-12.col-lg-12
						h2.txt-capital ახალი გეოსაინფორმაციო პაკეტი (მყარი)
	div.container(ng-controller="axaliMyariCtrl")
		form.form-horizontal
			div.col-md-12
				+panel('',4)
					+textbox([4, 4],'geoPaketi.ganacxadisID','განაცხადის ID','',true)
					+bigtext('geoPaketi._1_0','1 წიაღითსარგებლობის ობიექტი')
					+bigtext('geoPaketi._2_0','2 გენეტური ტიპი')
					+bigtext('geoPaketi._3_0','3 სასარგებლო წიაღისეულის სამრეწველო ტიპი')
