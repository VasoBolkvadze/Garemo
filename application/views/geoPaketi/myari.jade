							+fileUpload([4, 8],'geoPaketi._4_7/shapeFailebi','*',true,'შეიპ ფაილები',false)
						div.col-md-6
							+koordinatebisGridi()
						div.col-md-12
							+bigtext('geoPaketi._4_7.texti','ტექსტი')
					+bigtext('geoPaketi._4_8','4.8 ობიექტის აბსოლუტური სიმაღლე ზღვის დონიდან')
					+bigtext('geoPaketi._4_9','4.9 კლიმატური პირობები')
				+panel('5 ხელისშემშლელი ინფრასტრუქტურული ობიექტები და სხვა ფაქტორები',4)
					+bigtext('geoPaketi._5_1','5.1 მანძილი უახლოესი საავტომობილო გზის ღერძიდან')
