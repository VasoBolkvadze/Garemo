extends ../index.jade
block content
		.jumbotron.jumbotron-sm
			.container
				.row
					.col-sm-12.col-lg-12
						h2.txt-capital ახალი ლიცენზია
		.container
			.row
				.well
					.row
						form.form-horizontal
							div.col-md-7
								div.form-group
									label.control-label.col-md-4.txt-capital საიდენთიფიკაციო კოდი
